{
	"name": "lwc-auto-complete",
	"displayName": "LWC Auto Complete",
	"description": "Extension that automatically completes variables and functions declared in js files to HTML.",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.92.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"workspaceContains:force-app/main/default/lwc/*.js",
		"workspaceContains:force-app/main/default/lwc/*.html"
	],
	"main": "./dist/extension.js",
	"scripts": {
		"vscode:prepublish": "npm run package",
		"compile": "webpack",
		"watch": "webpack --watch",
		"package": "webpack --mode production --devtool hidden-source-map",
		"compile-tests": "tsc -p . --outDir out",
		"watch-tests": "tsc -p . -w --outDir out",
		"pretest": "npm run compile-tests && npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "vscode-test"
	},
	"devDependencies": {
		"@types/lodash": "^4.17.7",
		"@types/mocha": "^10.0.7",
		"@types/node": "20.x",
		"@types/vscode": "^1.92.0",
		"@typescript-eslint/eslint-plugin": "^7.14.1",
		"@typescript-eslint/parser": "^7.11.0",
		"@vscode/test-cli": "^0.0.9",
		"@vscode/test-electron": "^2.4.0",
		"eslint": "^8.57.0",
		"ts-loader": "^9.5.1",
		"typescript": "^5.4.5",
		"webpack": "^5.92.1",
		"webpack-cli": "^5.1.4"
	},
	"dependencies": {
		"@babel/parser": "^7.25.4",
		"@babel/traverse": "^7.25.4",
		"@types/babel__traverse": "^7.20.6",
		"loadash": "^1.0.0",
		"lodash": "^4.17.21",
		"ts-morph": "^23.0.0"
	},
	"publisher": "dev-yoonmoon"
}
